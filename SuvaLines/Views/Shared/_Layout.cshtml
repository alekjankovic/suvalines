<!DOCTYPE html>
<html lang="en-GB">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <meta name="description" content="Serbia Calling is informative platform which allows you to be informed">
    <meta name="keywords" content="News, Politics, Economy, Industry, Sport, Business, NewsLines, Europe, Serbia">
    <meta name="rating" content="general">

    <title>@ViewData["Title"] - SuvaLines</title>

    <!-- Include -->
    <link href="~/css/style.css" rel="stylesheet" type="text/css">
    <script src="~/js/jquery-3.2.1.min.js"></script>

    <!-- VUE Stuff -->
    @*<script src="~/plugins/vue/vue.js"></script>
    <script src="~/plugins/vue/axios.min.js"></script>*@
    <!-- VUE Stuff - END -->



    <link rel="stylesheet" href="~/plugins/font-awesome/css/font-awesome.min.css">

</head>
<body>

    @Html.Partial("_Header")

    <div id="main-app">
        @RenderBody()
    </div>

    @Html.Partial("_Footer")

    <script src="~/js/app_custom.js"></script>

    <!-- VUE Proj -->
    @*<script src="~/js/vue-app.js"></script>*@

    @RenderSection("Scripts", required: false)
</body>
</html>
